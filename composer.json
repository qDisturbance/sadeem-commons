{
  "name": "sadeem/commons",
  "description": "common modules used inside an api project",
  "keywords": [
    "api",
    "category",
    "model",
    "migration",
    "laravel"
  ],
  "homepage": "https://sadeem-tech.com",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Amro Elsabeihi",
      "email": "amrosubihie@gmail.com"
    }
  ],
  "require": {
    "goldspecdigital/laravel-eloquent-uuid": "^8.0",
    "spatie/laravel-activitylog": "^3.17",
    "spatie/laravel-permission": "^3.18"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "SadeemServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Sadeem\\Commons\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
